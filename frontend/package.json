{
  "name": "arba-delivery-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:enhanced": "node scripts/build-error-handler.js",
    "build:validate": "npm run build && npm run validate:assets",
    "build:config-validate": "node scripts/build-config-validator.js",
    "build:no-typecheck": "next build --no-lint && echo 'Build completed without TypeScript checking'",
    "build:analyze": "ANALYZE=true next build",
    "build:performance": "npm run build && node scripts/performance-audit.js",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "validate:assets": "node scripts/validate-assets.js",
    "validate:assets:production": "NODE_ENV=production node scripts/validate-assets.js",
    "performance:audit": "lighthouse http://localhost:3000 --output=json --output-path=./performance-report.json",
    "performance:build": "node scripts/performance-audit.js",
    "performance:runtime": "node scripts/runtime-performance-validator.js",
    "performance:validate": "npm run performance:build && npm run performance:runtime",
    "bundle:analyze": "npm run build:analyze",
    "validate:build": "node scripts/build-validation.js",
    "validate:build:full": "npm run validate:build && npm run build:validate",
    "deployment:validate": "node ../scripts/deployment-pipeline-validator.js",
    "deployment:validate:full": "npm run deployment:validate -- --verbose",
    "deployment:test": "node scripts/deployment-pipeline-test.js",
    "deployment:readiness": "node scripts/deployment-readiness-check.js",
    "deployment:full-test": "npm run deployment:readiness && npm run deployment:test",
    "validate:pre-build": "node scripts/pre-build-validation.js",
    "validate:health": "node scripts/deployment-health-check.js",
    "validate:continuous": "node scripts/continuous-validation.js",
    "validate:all": "npm run type-check && npm run lint",
    "test:validation-scripts": "node scripts/test-validation-scripts.js",
    "validate:ide": "node scripts/validate-ide-setup.js",
    "typescript:detect-errors": "node scripts/typescript-error-detector.js",
    "typescript:detect-errors:json": "node scripts/typescript-error-detector.js --format json",
    "typescript:detect-errors:summary": "node scripts/typescript-error-detector.js --format summary",
    "typescript:pre-commit": "node scripts/pre-commit-typescript-check.js",
    "typescript:validate": "node scripts/typescript-error-detector.js --no-fast-fail",
    "typescript:validate:critical": "node scripts/typescript-error-detector.js --format summary",
    "typescript:config:validate": "node scripts/typescript-config-manager.js validate",
    "typescript:config:paths": "node scripts/typescript-config-manager.js paths",
    "typescript:config:deps": "node scripts/typescript-config-manager.js deps",
    "typescript:config:optimize": "node scripts/typescript-config-manager.js optimize",
    "typescript:config:report": "node scripts/typescript-config-manager.js report",
    "typescript:performance:setup": "node scripts/typescript-performance-optimizer.js setup",
    "typescript:performance:validate": "node scripts/typescript-performance-optimizer.js validate",
    "typescript:performance:report": "node scripts/typescript-performance-optimizer.js report",
    "typescript:performance:clear": "node scripts/typescript-performance-optimizer.js clear",
    "typescript:performance:monitor": "node scripts/typescript-performance-optimizer.js validate && node scripts/typescript-performance-optimizer.js report",
    "monitoring:setup": "node scripts/typescript-monitoring-setup.js",
    "monitoring:collect": "node .typescript-monitoring/collect-metrics.js",
    "monitoring:trends": "node .typescript-monitoring/analyze-trends.js",
    "monitoring:alerts": "node .typescript-monitoring/check-alerts.js",
    "monitoring:dashboard": "next dev --port 3001",
    "type-check:monitored": "tsc --noEmit && npm run monitoring:collect",
    "build:monitored": "next build && npm run monitoring:collect",
    "integration:validate": "node scripts/validate-integration-compatibility.js",
    "integration:compatibility": "node scripts/validate-integration-compatibility.js",
    "workflow:validate": "npm run integration:validate && npm run typescript:validate",
    "workflow:compatibility": "npm run integration:compatibility",
    "type-check:fast": "tsc --noEmit --incremental --tsBuildInfoFile .typescript-cache/tsconfig.tsbuildinfo",
    "type-check:quick": "tsc --noEmit --skipLibCheck --incremental",
    "lint:fast": "eslint . --ext .ts,.tsx --cache --cache-location .typescript-cache/eslint-cache",
    "lint:quick": "eslint . --ext .ts,.tsx --cache --max-warnings 0 --quiet",
    "performance:benchmark": "node scripts/performance-tuning-optimizer.js",
    "performance:analyze": "node scripts/cicd-performance-optimizer.js",
    "cache:optimize": "node scripts/cache-optimization-manager.js",
    "cache:clean": ".typescript-cache/cleanup-cache.sh",
    "validate:fast": "npm run type-check:fast && npm run lint:fast",
    "validate:quick": "npm run type-check:quick && npm run lint:quick"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^3.3.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "critters": "^0.0.23",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.460.0",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.48.0",
    "socket.io-client": "^4.7.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.36",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "fast-check": "^4.5.2",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.0.0",
    "msw": "^2.12.4",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "web-vitals": "^4.2.4",
    "webpack-bundle-analyzer": "^4.10.1",
    "whatwg-fetch": "^3.6.20"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node scripts/pre-commit-typescript-check.js",
      "eslint --fix",
      "prettier --write",
      "bash -c 'tsc --noEmit'"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}